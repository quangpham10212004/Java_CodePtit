import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int tt = sc.nextInt();
        while(tt-- >0){
            long n = sc.nextLong();
            if((int)Math.sqrt(n)*(int)Math.sqrt(n) == n ){
                System.out.print("YES\n");
            }
            else{
                System.out.print("NO\n");
            }
        }
    }
}
