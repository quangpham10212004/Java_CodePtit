import java.math.BigInteger;
import java.util.*;

public class Main{
    static Scanner sc = new Scanner(System.in);
    
    
    public static void main(String[] args){
        int tt = sc.nextInt();
        while(tt-- >0){
            BigInteger a= new BigInteger(sc.next());
            BigInteger b = new BigInteger(sc.next());
            System.out.println(a.gcd(b));
        }
    
    }
    
}
